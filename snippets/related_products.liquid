{% assign collection = related_products %}

<div id="new-products" class="row">
  <div class="col-md-10 col-md-offset-1">
    <h2>{{ collection.title | split: ';' | last }}</h2>
    <ul class="products">
      {% for product in collection.all_products limit:5 %}
        {% include 'product' with product %}
      {% endfor %}
    </ul>
  </div>
  <div class="col-md-1 right"><div></div></div>
</div>
