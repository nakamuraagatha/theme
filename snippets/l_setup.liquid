{% capture dummy %}
  {% assign missing = '' %}
  {% assign locale = 'de' %}
  {% assign locale_prefix = ''  %}
  {% assign ti = 0 %}
  {% if request.host contains 'playmakers-football.com' %}
    {% assign locale = 'en' %}
    {% assign locale_prefix = locale | append: '.'  %}
    {% assign ti = 1 %}
  {% endif %}
  {% comment %}{{'aaaaa' | split: '|' | slice: ti}}{% endcomment %}
{% endcapture %}